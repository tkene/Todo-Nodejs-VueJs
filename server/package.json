{
  "name": "todo-tags-backend",
  "version": "1.0.0",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "migrate": "node scripts/migrate-db.js",
    "db:migrate": "sequelize-cli db:migrate",
    "db:migrate:undo": "sequelize-cli db:migrate:undo",
    "setup:production": "node scripts/setup-production.js",
    "backup:db": "node scripts/backup-database.js",
    "download:db": "node scripts/download-database.js",
    "restore:db": "node scripts/restore-database.js",
    "test:scraper": "node scripts/test-scraper.js",
    "inspect:pmu": "node scripts/inspect-pmu-page.js",
    "test:boturfers": "node scripts/test-boturfers.js",
    "test:bettracker": "node scripts/test-bettracker.js",
    "test:bettracker:races": "node scripts/test-bettracker.js --endpoint=races",
    "test:bettracker:quinte": "node scripts/test-bettracker.js --endpoint=quinte"
  },
  "dependencies": {
    "axios": "^1.13.2",
    "bcryptjs": "^3.0.3",
    "cheerio": "^1.1.2",
    "connect-sqlite3": "^0.9.16",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^4.18.2",
    "express-session": "^1.18.2",
    "sequelize": "^6.37.7",
    "sequelize-cli": "^6.6.3",
    "sqlite3": "^5.1.7"
  },
  "devDependencies": {
    "nodemon": "^3.1.11"
  }
}
